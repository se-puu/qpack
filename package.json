    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "terser-webpack-plugin": "^5.3.12",
    "ts-loader": "^9.5.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
